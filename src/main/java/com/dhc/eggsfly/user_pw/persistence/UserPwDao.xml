<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dhc.eggsfly.user_pw.persistence.UserPwDao">
<update id="updateUserPw" parameterType="com.dhc.eggsfly.user_pw.domain.UserPw">

    UPDATE
    user_information
    
    SET
    password=#{password}
    
    WHERE
    user_id=#{user_id}

</update>

</mapper>